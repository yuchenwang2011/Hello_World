<?xml version="1.0" encoding="UTF-8"?>
<!--Add a letter prefix in front to call namespace to avoid naming conflicts -->
<!--You can call namespace at the root node-->
<!--Syntax: xmlns:prefix="URI" -->
<!--Note: The namespace URI is not used by the parser to look up information.-->
<!-- The purpose of using an URI is to give the namespace a unique name. -->
<root xmlns:h="http://www.w3.org/TR/html4/"
xmlns:f="https://www.w3schools.com/furniture">
xmlns:f="">
  <h:table>
    <h:tr>
      <h:td>Apples</h:td>
      <h:td>Bananas</h:td>
    </h:tr>
  </h:table>
  
  <f:table>
    <f:name>African Coffee Table</f:name>
    <f:width>80</f:width>
    <f:length>120</f:length>
  </f:table>
  
  <!--XLink to create a hyperlink in xml file-->
  <homepages xmlns:xlink="http://www.w3.org/1999/xlink">
    <homepage xlink:type="simple" xlink:href="https://www.w3schools.com">Visit W3Schools</homepage>
    <homepage xlink:type="simple" xlink:href="http://www.w3.org">Visit W3C</homepage>
  </homepages>
</root>
